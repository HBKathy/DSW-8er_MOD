SELECT temp_table_alle_kt_dsw_mod8.*
FROM temp_table_alle_kt_dsw_mod8
LEFT JOIN temp_table_alle_kt_dsw_mod8_techa
ON temp_table_alle_kt_dsw_mod8.teilenr = temp_table_alle_kt_dsw_mod8_techa.teilenr
WHERE temp_table_alle_kt_dsw_mod8_techa.teilenr IS NULL

UNION

-- Zeilen aus Tabelle2, deren teilenr nicht in Tabelle1 vorkommt
SELECT temp_table_alle_kt_dsw_mod8_techa.*
FROM temp_table_alle_kt_dsw_mod8_techa
LEFT JOIN temp_table_alle_kt_dsw_mod8
ON temp_table_alle_kt_dsw_mod8_techa.teilenr = temp_table_alle_kt_dsw_mod8.teilenr
WHERE temp_table_alle_kt_dsw_mod8.teilenr IS NULL;
